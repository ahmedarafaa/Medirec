@model IEnumerable<MediRec.Models.Immunizations>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Vaccines.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateGiven)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdministratedBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NextDoesDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Vaccines.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateGiven)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdministratedBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NextDoesDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ImmunizationId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ImmunizationId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ImmunizationId })
        </td>
    </tr>
}

</table>
